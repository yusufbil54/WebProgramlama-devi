@model List<WebApplication15.Data.User>
@{
    Layout = "~/Views/Shared/_adminLayout.cshtml";
    ViewData["panel"]="User";
}
<!DOCTYPE html>
<html lang="en">
<head>
    
    <style>
       

        h2 {
            margin-bottom: 20px;
        }

        table {
            width: 70%;
            border-collapse: collapse;
            margin-left:17%;

        }

        th, td {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }

        th {
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <section class="contact section-padding">
  <table>
<thead>
    <tr>
    <th>Id</th>
    <th>Name</th>
    <th>SurName</th>
    <th> </th>
    </tr >
</thead>
<tbody>
    @foreach(var AddUserViewModel in Model)
    {
       <tr>
                <td> @AddUserViewModel.Id</td>
                   <td> @AddUserViewModel.UserName</td>
                       <td> @AddUserViewModel.UserSurname</td>
                               <td> <a href="Login/View/@AddUserViewModel.Id">View</a> </td>
        </tr>
    }
</tbody>
  </table>
  </section>
  </body>
</html>

